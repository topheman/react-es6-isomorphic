<!DOCTYPE html>
<html>
<head lang="en">
  <meta charset="UTF-8">
  <title>Topheman - react-es6-isomorphic</title>
  <% if(env === 'PROD') { %>
  <link rel="stylesheet" type="text/css" href="/assets/css/main.css">
  <% } else { %>
  <link rel="stylesheet" type="text/css" href="http://localhost:8080/assets/css/main.css">
  <% } %>
  <meta name="viewport" content="initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
</head>
<body>
<% include ./partials/analytics %>
<div><%- markup %></div>
<% if(typeof serializedState !== 'undefined') { %>
<script type="text/javascript">
var __DATA__ = <%- serializedState %>;
</script>
<% } %>
<% if(typeof error === 'undefined') { %>
  <% if(env === 'PROD') { %>
  <script src="/assets/js/bundle.js"></script>
  <% } else { %>
  <script src="http://localhost:8080/webpack-dev-server.js"></script>
  <script src="http://localhost:8080/assets/js/bundle.js"></script>
  <% } %>
  <!-- twitter button -->
  <script>!function (d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0], p = /^http:/.test(d.location) ? 'http' : 'https';
      if (!d.getElementById(id)) {
        js = d.createElement(s);
        js.id = id;
        js.src = p + '://platform.twitter.com/widgets.js';
        fjs.parentNode.insertBefore(js, fjs);
      }
    }(document, 'script', 'twitter-wjs');</script>
<% } %>
</body>
</html>